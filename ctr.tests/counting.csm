#alloc 5
#const print_t str "Hooray!\n"
#const line_b str "\n"

#const zero i64 0
#const one i64 1
#const goal i64 255

success:
    load 3 print_t
    ret 3

incr:
    add 1 1 2
    str.from 3 1
    load 4 line_b
    add 3 3 4
    str.echo 3
compare:
    eq 0 0 1
    jmp incr:
    jmp success:

main:
    load 0 goal
    load 1 zero
    load 2 one
    jmp compare:
